    "    print(results[['Term', 'Adjusted P-value', 'Genes']].head(10))\n",
    "    \n",
    "    # Visualize\n",
    "    top_terms = results.head(15)\n",
    "    \n",
    "    plt.figure(figsize=(12, 8))\n",
    "    plt.barh(range(len(top_terms)), -np.log10(top_terms['Adjusted P-value']), \n",
    "             color='steelblue')\n",
    "    plt.yticks(range(len(top_terms)), top_terms['Term'], fontsize=10)\n",
    "    plt.xlabel('-Log10(Adjusted P-value)', fontweight='bold')\n",
    "    plt.title('Top Enriched GO Biological Processes', fontsize=14, fontweight='bold')\n",
    "    plt.axvline(-np.log10(0.05), color='red', linestyle='--', label='p=0.05')\n",
    "    plt.legend()\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
