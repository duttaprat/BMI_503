  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Filter high-quality variants\n",
    "quality_threshold = 50\n",
    "depth_threshold = 100\n",
    "\n",
    "high_quality_variants = df_variants[\n",
    "    (df_variants['QUAL'] >= quality_threshold) &\n",
    "    (df_variants['DP'] >= depth_threshold) &\n",
    "    (df_variants['FILTER'] == 'PASS')\n",
    "]\n",
    "\n",
    "print(f\"High-quality variants (Q>={quality_threshold}, DP>={depth_threshold}):\")\n",
    "print(high_quality_variants[['CHROM', 'POS', 'ID', 'REF', 'ALT', 'QUAL', 'GENE', 'EFFECT']])\n",
    "print(f\"\\nFiltered {len(high_quality_variants)} out of {len(df_variants)} variants\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Filter by allele frequency\n",
    "print(\"Variants by Allele Frequency:\")\n",
    "print(\"\\nCommon variants (AF > 0.05):\")\n",
    "common = df_variants[df_variants['AF'] > 0.05]\n",
    "print(f\"  Count: {len(common)}\")\n",
    "\n",
    "print(\"\\nRare variants (AF < 0.01):\")\n",
    "rare = df_variants[df_variants['AF'] < 0.01]\n",
    "print(f\"  Count: {len(rare)}\")\n",
    "\n",
    "print(\"\\nIntermediate frequency (0.01 <= AF <= 0.05):\")\n",
    "intermediate = df_variants[(df_variants['AF'] >= 0.01) & (df_variants['AF'] <= 0.05)]\n",
    "print(f\"  Count: {len(intermediate)}\")"
