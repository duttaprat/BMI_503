    "# 4. Heatmap: Effect type vs Quality category\n",
    "# Categorize quality scores\n",
    "df_variants['QUAL_Category'] = pd.cut(df_variants['QUAL'], \n",
    "                                       bins=[0, 50, 100, 250], \n",
    "                                       labels=['Low', 'Medium', 'High'])\n",
    "\n",
    "# Create contingency table\n",
    "contingency = pd.crosstab(df_variants['EFFECT'], df_variants['QUAL_Category'])\n",
