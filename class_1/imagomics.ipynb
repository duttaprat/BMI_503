    "# Create a synthetic H&E-like tissue image\n",
    "# This simulates what an H&E stained tissue looks like\n",
    "\n",
    "def create_synthetic_he_image(size=400):\n",
    "    \"\"\"Create a synthetic H&E-like image with tissue and nuclei\"\"\"\n",
    "    \n",
    "    # Background (whitish)\n",
    "    img = np.ones((size, size, 3)) * 240\n",
    "    \n",
    "    # Tissue region (pinkish - eosin)\n",
    "    tissue_mask = np.zeros((size, size))\n",
    "    tissue_mask[100:350, 50:350] = 1\n",
    "    img[tissue_mask == 1] = [220, 180, 200]  # Pink color\n",
    "    \n",
    "    # Add some nuclei (bluish - hematoxylin)\n",
    "    np.random.seed(42)\n",
    "    n_nuclei = 30\n",
    "    for _ in range(n_nuclei):\n",
    "        y = np.random.randint(120, 330)\n",
    "        x = np.random.randint(70, 330)\n",
    "        radius = np.random.randint(5, 10)\n",
    "        \n",
    "        # Create circular nucleus\n",
    "        yy, xx = np.ogrid[:size, :size]\n",
    "        mask = (yy - y)**2 + (xx - x)**2 <= radius**2\n",
    "        img[mask] = [100, 100, 180]  # Blue-purple color\n",
    "    \n",
    "    # Add some noise for realism\n",
    "    noise = np.random.normal(0, 5, img.shape)\n",
    "    img = np.clip(img + noise, 0, 255)\n",
    "    \n",
    "    return img.astype(np.uint8)\n",
    "\n",
    "# Create synthetic H&E image\n",
    "he_image = create_synthetic_he_image()\n",
    "\n",
    "# Display\n",
    "plt.figure(figsize=(8, 8))\n",
    "plt.imshow(he_image)\n",
    "plt.title('Synthetic H&E Tissue Image', fontweight='bold', fontsize=14)\n",
    "plt.axis('off')\n",
    "plt.show()\n",
    "\n",
    "print(\"âœ… Created synthetic H&E image for analysis\")\n",
    "print(f\"Image dimensions: {he_image.shape}\")"
   ]
