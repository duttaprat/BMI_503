    "# Step 2: Nucleus Detection\n",
    "\n",
    "def detect_nuclei(image, min_size=20, max_size=500):\n",
    "    \"\"\"Detect and count nuclei in H&E image\"\"\"\n",
    "    \n",
    "    # Extract blue channel (hematoxylin stains nuclei)\n",
    "    blue_channel = image[:, :, 2]\n",
    "    \n",
    "    # Invert: nuclei are dark, we want them bright\n",
    "    blue_inv = 255 - blue_channel\n",
    "    \n",
    "    # Threshold to find nuclei\n",
    "    threshold = filters.threshold_otsu(blue_inv)\n",
    "    nuclei_mask = blue_inv > threshold\n",
    "    \n",
    "    # Clean up\n",
    "    nuclei_mask = morphology.remove_small_objects(nuclei_mask, min_size=min_size)\n",
    "    nuclei_mask = morphology.remove_small_holes(nuclei_mask, area_threshold=50)\n",
    "    \n",
    "    # Label individual nuclei\n",
    "    labeled_nuclei = measure.label(nuclei_mask)\n",
    "    \n",
    "    # Filter by size\n",
    "    props = measure.regionprops(labeled_nuclei)\n",
    "    valid_labels = [prop.label for prop in props if min_size < prop.area < max_size]\n",
    "    \n",
    "    # Create final mask with only valid nuclei\n",
    "    final_mask = np.isin(labeled_nuclei, valid_labels)\n",
    "    final_labeled = measure.label(final_mask)\n",
    "    \n",
    "    return final_labeled\n",
    "\n",
    "# Detect nuclei\n",
    "nuclei_labeled = detect_nuclei(he_image)\n",
    "n_nuclei = nuclei_labeled.max()\n",
    "\n",
    "print(f\"âœ… Detected {n_nuclei} nuclei\")"
   ]
